class classCar:
	def __init__(self, fuelType, numOfSeats, milagePerHour):
		self.fuelType = fuelType
		self.numOfSeats = numOfSeats
		self.milagePerHour = milagePerHour
	
	def tellFuelType(self):
		print("The fuel type is" + self.fuelType)
	
car1 = classCar("petrol", 6, 40)


car1.tellFuelType() 

print(car1.fuelType)
print(car1.numOfSeats)
print(car1.milagePerHour)
